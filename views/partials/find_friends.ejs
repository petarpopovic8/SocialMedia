<li class="collection-header"><h6>Find friends</h6></li>
<%users.forEach(function(friend){%>
<li class="collection-item avatar">
	<i class="material-icons circle">face</i>
	<span class="title"><%=friend.name%></span>
	<p><%=friend.user_profile[0].location%></p>
	<div style="cursor: pointer;" class="secondary-content"><i class="material-icons add_friend" onclick='add_friend(this.id)' id="<%=friend.member_id%>">person_add</i></div>
</li>
<%})%>	