
<div class="row">
	<div class="col s6"> 
		<ul class="collection">
			<%friends.forEach(function(friend){%>
			<li class="collection-item avatar" id="<%=friend.member_id%>">
				<%if (friend.profile_pic != "default_profile.png") {%>
				<img src="http://localhost:3000/user_profile_images/<%=friend.profile_pic%>" alt="" class="circle">

			
				<%} else {%>
				<i class="material-icons circle">face</i>
				<%}%>
				<a href='http://localhost:3000/user_profile/<%=friend.member_id%>'><span class="title"><%=friend.friend_name%></span></a>
				<p><%=friend.location%></p>
				<!-- <button class="chat_button" type="button" onclick="openChat()" id="<%=friend.member_id%>">Chat</button> -->
			</li>
			<%})%>
		</ul>
	</div>
</div>

